<div class="container">
    <h1 class="mt-5 mb-4">Coffee Menu</h1>

        
        <div class="row">

        <!-- Coffee Card -->
        <div class="col-md-4">
            <div class="card coffee-card">
                <img src="data:image/png;base64," class="card-img-top" alt="Coffee 1">
                <div class="card-body">
                    <h5 class="card-title">Coffee Name 1</h5>
                    <p class="card-text">Description of Coffee 1.</p>
                    <p class="card-text">Price: $5.99</p>
                    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#buyModal1">Buy</button>
                </div>
            </div>
        </div>
        </div>
        } 
       
        <!-- Add more coffee cards as needed -->
   
</div>










 

    <!-- Button to trigger the modal -->
    <button type="button" class="btn btn-lg btn-buy" data-toggle="modal" data-target="#coffeeBillingModal">
        Buy Coffee
    </button>

    <!-- Modal -->
<div class="modal fade" id="buyModal1" tabindex="-1" role="dialog" aria-labelledby="coffeeBillingModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header bg-dark text-white">
                    <h5 class="modal-title" id="coffeeBillingModalLabel">Coffee Billing</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                <div class="form-group">
                   
                    <label for="Customerphon">Phone:</label>
                    <input type="text" class="form-control" id="Customerphon" value="">
                </div>
                <div class="form-group">
                    <label for="prv">Month of Coffie:</label>
                    <input type="text" class="form-control" id="monthofcoffie" value="20" readonly>
                </div>
                    <div class="form-group">
                        <label for="coffeeName">Coffee Name:</label>
                        <input type="text" class="form-control" id="coffeeName" value="Espresso" readonly>
                    </div>



                    <div class="form-group">
                        <label for="coffeePrice">Price:</label>
                        <input type="text" class="form-control" id="coffeePrice" value="$2.50" readonly>
                    </div>
                    <div class="form-group">
                        <label for="quantity">Quantity:</label>
                        <input type="number" class="form-control" id="quantity" value="1" min="1">
                    </div>
                    <div class="form-group">
                        <label for="total">Total:</label>
                        <input type="text" class="form-control" id="total" readonly>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-buy" id="confirmOrder">Confirm Order</button>
                </div>
            </div>
        </div>
    </div>
<!-- JavaScript Libraries -->
 
<script src="~/lib/easing/easing.min.js"></script>
<script src="~/lib/waypoints/waypoints.min.js"></script>
<script src="~/lib/owlcarousel/owl.carousel.min.js"></script>
<script src="~/lib/tempusdominus/js/moment.min.js"></script>
<script src="~/lib/tempusdominus/js/moment-timezone.min.js"></script>
<script src="~/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

<!-- Contact Javascript File -->
<script src="~/mail/jqBootstrapValidation.min.js"></script>
<script src="~/mail/contact.js"></script>

<!-- Template Javascript -->
<script src="~/js/main.js"></script>
  
  <script>
        // JavaScript code for calculating total based on quantity and coffee price
        $(document).ready(function() {
          // Set coffee name and price when a coffee is selected
          $('#confirmOrder').click(function() {
            var coffeePrice = parseFloat($('#coffeePrice').val().replace('$', ''));
            var quantity = parseInt($('#quantity').val());
            var total = coffeePrice * quantity;
            $('#total').val('$' + total.toFixed(2));
          });
        });
    </script>
